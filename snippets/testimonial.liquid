<div class="testimonial">
    <div class="testimonial__img-wrap">
        <img class="testimonial__img" src="img/testimonial.jpg" alt="" width="140px" height="140px">
    </div>
    <div class="testimonial__stars">
        {% render 'ico-star' %}
        {% render 'ico-star' %}
        {% render 'ico-star' %}
        {% render 'ico-star' %}
        {% render 'ico-star' %}
    </div>
    <div class="testimonial__title">{{ title }}</div>
    <p class="testimonial__text">{{ textdata }}</p>
    <div class="testimonial__name">{{ name }}</div>
    <a href="#" class="testimonial__product">
        <img class="testimonial__product-img" src="img/product.png" alt="" width="92px" height="70px">
        <div class="testimonial__product-name"></div>
        <span class="testimonial__product-arr">
            {% render 'ico-arr-r' %}
        </span>
    </a>
</div>
